QWidget {
    background-color: #303030;
}

QPushButton {
    font-weight: bold;
    font-size: 16px;
    color: #303030;
    border: 2px solid #202020;
    border-radius: 8px;
    min-width: 10px;
    background-color: #FF7C0A;
}

QPushButton#pushButton {
    font-size: 20px;
}

QPushButton:hover {
    background-color: #eb6200;
}

QLabel {
    color: #F9F6F0;
    font-weight: bold;
    font-size: 14px;
}

QLineEdit {
    padding: 1px;
    color: #202020;
    border: 2px solid #aaa;
    background-color: #EEE;
    font-weight: bold;
    font-size: 14px;
}

QGroupBox {
    font-weight: bold;
    font-size: 15px;
    color: #F9F6F0;
    border-radius: 8px;
}

QComboBox {
    font-weight: bold;
    font-size: 15px;
    color: #F9F6F0;
    border-radius: 50px;
}

QComboBox::drop-down {
    font-weight: bold;
    font-size: 15px;
    color: #F9F6F0;
    width: 20px;
    height: 30px;
}

QComboBox QAbstractItemView {
    border: 2px solid #F9F6F0;
    color: #F9F6F0;
    selection-background-color: #eb6200;
    padding: 2px 8px 2px 8px;
    font-size: 20px;
}

QLCDNumber {
    border: 2px solid #F9F6F0;
    color: #F9F6F0;
}

QTabBar {
    font-weight: bold;
    font-size: 20px;
    color: #121212;
    border-radius: 8px;
}

QTabBar::tab:selected {
    background-color: #Ff7f1f;
    border: 3px solid #202020;
    border-radius: 8px;
    min-height: 50px;
    min-width: 160px;
}

QTabBar::tab:!selected {
    background-color: #eF6F0F;
    border: 2px solid #202020;
    border-radius: 8px;
    min-height: 50px;
    min-width: 160px;
}

QRadioButton {
    color: #F9F6F0;
    font-weight: bold;
    font-size: 14px;
}